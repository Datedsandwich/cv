{
  "name": "react-playground",
  "version": "0.0.1",
  "dependencies": {
    "file-loader": "^1.1.11",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack-dev-server --open",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(svg)$": "<rootDir>/__mocks__/svgMock.js"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "setupFiles": [
      "./jestSetup.js"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "jest": "^22.4.3",
    "mini-css-extract-plugin": "^0.3.0",
    "node-sass": "^4.8.3",
    "sass-loader": "^6.0.7",
    "webpack": "^4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
